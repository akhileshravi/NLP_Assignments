   "source": [
    "def num_meanings(word):\n",
    "    synonyms = []\n",
    "    antonyms = []\n",
    "\n",
    "    for syn in wordnet.synsets(word):\n",
    "        for l in syn.lemmas():\n",
    "            synonyms.append(l.name())\n",
    "            if l.antonyms():\n",
    "                 antonyms.append(l.antonyms()[0].name())\n",
    "\n",
    "    return len(list(set(synonyms)))"
   ]
